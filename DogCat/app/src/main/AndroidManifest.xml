<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.midtwenties.dogcat">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Lockermain"/>
        <activity android:name=".HangoutPopup"/>
        <activity android:name=".HospitalActivity" />
        <activity android:name=".HospitalTreatment" />
        <activity android:name=".HospitalRoom" />
        <activity android:name=".MissionActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".PetIDActivity" />
        <activity android:name=".PlayActivity"/>
        <activity android:name=".InfoNext" />
        <activity android:name=".ItemList"/>
        <activity android:name=".FeedActivity" />
        <activity android:name=".AttendancePopup" android:theme="@style/pop"/>
        <activity android:name=".StoreItem" />
        <activity android:name=".InitPet" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".TermsOfService" />
        <activity android:name=".YardActivity" />
        <activity android:name=".WashActivity"/>
        <activity android:name=".MainActivity" />
        <activity android:name=".MainSetting" />
        <activity android:name=".IDListActivity" />
        <receiver
            android:name=".Receiver"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.SCREEN_OFF"/>
            </intent-filter>
        </receiver>

        <!--<service
            android:name=".ScreenService"
            android:enabled="true"
            android:exported="false">
        </service>
        -->
    </application>
</manifest>