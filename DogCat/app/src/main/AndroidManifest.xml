<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.midtwenties.dogcat">
    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/dogfoot"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dogfoot"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".PetIDActivity"/>
        <activity android:name=".InfoNext"/>
        <activity android:name=".FeedActivity" />
        <activity android:name=".InitPet"/>
        <activity android:name=".InfoActivity"/>
        <activity android:name=".TermsOfService"/>
        <activity android:name=".YardActivity"></activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".MainSetting" />
        <activity android:name=".StoreItem"/>
        <activity android:name=".AttendancePopup"/>
        <activity android:name=".IDListActivity"></activity>
    </application>
</manifest>